{
  "reporterEnabled": "mocha-junit-reporter, mochawesome, mocha-jenkins-reporter",
  "mochaJenkinsReporterReporterOptions": {
    "junit_report_path": "reports/jenkins-test-results.xml",
    "junit_report_toConsole": false
  },
  "mochaJunitReporterReporterOptions": {
    "mochaFile": "reports/junit-test-results.xml",
    "jenkinsMode": true,
    "includePending": true
  },
  "mochawesomeReporterOptions": {
    "reportFilename":"mocha-test-results",
    "reportDir":"reports",
    "inlineAssets": true,
    "overwrite": true,
    "quiet": true
  }
}