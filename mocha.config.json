{
  "reporterEnabled": "mocha-jenkins-reporter, mochawesome",
  "reporterOptions": {
    "junit_report_path": "reports/jenkins-test-results.xml",
    "junit_report_stack": 1
  },
  "mochawesomeReporterOptions": {
    "reportFilename":"mocha-test-results",
    "reportDir":"reports",
    "inlineAssets": true,
    "overwrite": true,
    "quiet": true
  }
}